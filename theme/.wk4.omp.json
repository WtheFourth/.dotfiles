{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "blue": "#89B4FA",
    "closer": "p:os",
    "lavender": "#B4BEFE",
    "os": "#ACB0BE",
    "pink": "#F5C2E7",
    "green": "#A6E3A1"
  },
  "upgrade": {
    "source": "cdn",
    "interval": "168h",
    "auto": false,
    "notice": false
  },
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "\uf105 "
  },
  "blocks": [
    {
      "newline": true,
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "{{.Icon}} ",
          "foreground": "p:os",
          "type": "os",
          "style": "plain"
        },
        {
          "properties": {
            "home_icon": "~",
            "style": "powerlevel",
            "max_width": 4
          },
          "template": "{{ .Path }} ",
          "foreground": "p:pink",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "branch_icon": "\ue725 ",
            "branch_ahead_icon": "\ueaa1 ",
            "branch_behind_icon": "\uea9a ",
            "branch_identical_icon": "\uf0c9 ",
            "branch_gone_icon": "\udb80\udea0 ",
            "no_commits_icon": "\uf0c3 ",
            "modified_icon": "M",
            "added_icon": "A",
            "deleted_icon": "D",
            "renamed_icon": "R",
            "conflicted_icon": "C"
          },
          "template": "{{ .UpstreamIcon }}{{ if .HEAD }} {{ .HEAD }}{{ end }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}",
          "foreground": "p:lavender",
          "type": "git",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "ruby",
          "style": "plain",
          "template": "\ue23e {{.Full}}",
          "foreground": "p:pink"
        },
        {
          "type": "dotnet",
          "style": "plain",
          "template": "\uf292 {{.Full}}",
          "foreground": "p:green"
        },
        {
          "type": "react",
          "style": "plain",
          "template": "\ue7ba {{.Full}}"
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "p:closer",
          "properties": {
            "time_format": "3:04:05 pm"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "template": "\uf105",
          "foreground": "p:closer",
          "type": "text",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
